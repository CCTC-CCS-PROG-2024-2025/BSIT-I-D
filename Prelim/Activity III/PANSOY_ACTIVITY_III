import java.until.Scanner;

public class PANSOY_ACTIVITY_III{
    public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        System.out.printIn("Enter Your Celsius: ");
        int temp = sc.nextInt();

        if (temp>=150);{
            System.out.printIn("Boiling");
        } else {
            System.out.printIn("Not Boiling")
        }
        } 
    }
